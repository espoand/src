<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.71" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.ui.controller.GestioneContrattoController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
         <menus>
            <Menu mnemonicParsing="false" text="File">
              <items>
                <MenuItem mnemonicParsing="false" text="Quit" />
              </items>
            </Menu>
         </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="286.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
      <children>
            <Button layoutX="20.0" layoutY="86.0" mnemonicParsing="false" onAction="#aggiungi" text="Aggiungi Contratto" />
            <Button layoutX="176.0" layoutY="86.0" mnemonicParsing="false" onAction="#modifica" prefHeight="27.0" prefWidth="135.0" text="Modifica Contratto" />
            <Button layoutX="330.0" layoutY="86.0" mnemonicParsing="false" onAction="#elimina" prefHeight="27.0" prefWidth="135.0" text="Elimina Contratto" />
            <Button layoutX="492.0" layoutY="86.0" mnemonicParsing="false" onAction="#indietro" prefHeight="27.0" prefWidth="119.0" text="Torna alla Home" />
            <Button layoutX="489.0" layoutY="35.0" mnemonicParsing="false" onAction="#cerca" prefHeight="27.0" prefWidth="119.0" text="Cerca Contratto" />
            <TextField fx:id="nroOrdine" layoutX="14.0" layoutY="35.0" prefHeight="27.0" prefWidth="364.0" text="Numero Ordine" />
            <TableView fx:id="listaContratti" layoutX="20.0" layoutY="131.0" prefHeight="200.0" prefWidth="588.0">
              <columns>
                <TableColumn fx:id="tabNroOrdine" editable="false" prefWidth="173.0" sortable="false" text="Numero Ordine" />
                <TableColumn fx:id="tabCf" editable="false" minWidth="0.0" prefWidth="245.0" sortable="false" text="Codice Fiscale " />
                  <TableColumn fx:id="tabTarga" editable="false" prefWidth="168.0" sortable="false" text="Targa Auto Noleggiata" />
              </columns>
            </TableView>
      </children>
    </AnchorPane>
  </children>
</VBox>
